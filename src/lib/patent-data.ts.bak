export function getPatentData(shortName: string): PatentAnalysis | null {
  return PATENT_DATA[shortName] || null
}

export function getAllPatents(): PatentAnalysis[] {
  return Object.values(PATENT_DATA).sort((a, b) => 
    a.patent_number.localeCompare(b.patent_number)
  )
}
